#include <stdio.h>

int main() {
    int array[] = {1, 2, 3, 4, 5}; // Sample array
    int n = sizeof(array) / sizeof(array[0]); // Calculate the number of elements in the array
    int sum = 0; // Initialize a variable to store the sum

    // Use a loop to iterate through the array and calculate the sum
    for (int i = 0; i < n; i++) {
        sum += array[i]; // Add the current element to the sum
    }

    printf("The sum of elements in the array is: %d\n", sum);

    return 0;
}
